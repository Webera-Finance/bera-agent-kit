{
  "name": "bera-agent-kit",
  "version": "1.0.0",
  "description": "Open-source agent kit for interacting with Berachain protocols",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Webera-Finance/bera-agent-kit"
  },
  "keywords": [
    "berachain",
    "agent",
    "blockchain",
    "defi",
    "automation"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npx mocha -r ts-node/register 'test/**/*.spec.ts'",
    "test:file": "npx mocha -r ts-node/register",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "typedoc src --out docs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^1.0.0",
    "@ibm-cloud/watsonx-ai": "^1.3.0",
    "@langchain/community": "^0.3.22",
    "@langchain/core": "^0.3.27",
    "@langchain/langgraph": "^0.2.39",
    "@playwright/test": "^1.42.1",
    "axios": "^1.7.9",
    "chai": "^4.3.7",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.7",
    "ibm-cloud-sdk-core": "^5.1.0",
    "langsmith": "^0.2.14",
    "mocha": "^11.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.73.1",
    "sinon": "^19.0.2",
    "typedoc": "^0.27.6",
    "viem": "^2.21.57",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  },
  "resolutions": {
    "whatwg-url": "^14.1.0"
  }
}